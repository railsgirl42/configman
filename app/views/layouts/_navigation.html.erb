<nav>
  <ul>
    <li>
     <%= link_to "Home", root_path, :class => 'brand' %>
    </li>
    <% if logged_in? %>
      <li>
        <%= link_to 'Logout', logout_path %>
      </li>
      <li>
        <%= link_to 'Edit account', edit_user_path(current_user) %>
      </li>
    <% else %>
      <li>
        <%= link_to 'Login', login_path %>
      </li>
      <li>
        <%= link_to 'Sign up', new_user_path %>
      </li>
    <% end %>
  </ul>
</nav>
