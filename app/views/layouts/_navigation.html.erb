  <ul>
    <li>
     <%= link_to "Home", root_path %>
    </li>
    <% if logged_in? %>
      <li>
        <%= link_to 'Logout', logout_path %>
      </li>
      <li>
        <%= link_to 'Edit account', edit_user_path(current_user), class: 'dialog' %>
